GET http://localhost:8080/contacts/687aa30e0fbc489862c7b163

### Create a new contact
POST http://localhost:8080/contacts
Content-Type: application/json

{
  "testType": "personality",
  "testName": "PAI",
  "questions": [
    {
      "question": "Are you comfortable in social situations?",
      "questionType": "single choice",
      "options": [
        {
          "optionNumber": 0,
          "option": "Strongly Disagree",
          "isCorrect": false
        },
        {
          "optionNumber": 1,
          "option": "Disagree",
          "isCorrect": false
        },
        {
          "optionNumber": 2,
          "option": "Neutral",
          "isCorrect": false
        }
      ]
    },
    {
      "question": "Describe your biggest strength",
      "questionType": "text",
      "options": []
    }
  ]
}


